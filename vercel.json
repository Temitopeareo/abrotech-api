{
  "version": 2,
  "builds": [
    {
      "src": "api/upload-file.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/chatgpt-4.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/freefire-stalk.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/reminiByabro.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/docs.js" 
    },
    {
      "src": "/docs",
      "dest": "/api/docs.js"
    },
    {
      "src": "/api/upload-file",
      "dest": "/api/upload-file.js"
    },
    {
      "src": "/api/chatgpt-4",
      "dest": "/api/chatgpt-4.js"
    },
    {
      "src": "/api/freefire-stalk",
      "dest": "/api/freefire-stalk.js"
    },
    {
      "src": "/api/reminiByabro",
      "dest": "/api/reminiByabro.js"
    }
  ]
}

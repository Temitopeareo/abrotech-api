{
  "version": 2,
  "builds": [
    {
      "src": "api/upload-file.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/upload-file",
      "dest": "/api/upload-file.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}

